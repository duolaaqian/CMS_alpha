<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="mybatis.valuelist">
	<select id="getUser_rr" parameterType="String" resultType="java.util.Map">
		select * from oa_basic_ug_user where id_=#{id} and 1=2
	</select>
	<select id="getAllUser_rr" resultType="java.util.Map">
		select * from oa_basic_ug_user
	</select>
</mapper>